terminology
    term_definitions = <
        ["de"] = <
            ["id1"] = <
                text = <"Apgar-Score">
                description = <"Klinischer Score, abgeleitet von der Beurteilung des Atemantriebs, Herzfrequenz, Reflexauslösbarkeit, Muskeltonus und Hautfarbe.">
            >
            ["id2"] = <
